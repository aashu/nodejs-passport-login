<h1>Hello, <%= name %>!</h1>
<h3>Here is some secret, dont share it with anybody else.</h3>

<h1 id="heading"></h1>
<script>
  const secret = document.getElementById("heading");
  fetch("https://api.quotable.io/random?tags=technology")
    .then((response) => response.json())
    .then((data) => (secret.innerText = data.content));
</script>
